extend base.pug

block script
  script(src='/static/script.js')

block style
  block style
    style
      include ../static/contact.css
      include ../static/style.css

block content
  h1 Contact Us
  form(action='/contact',method='post',class='Form')
    input(type='text',name='Name',placeholder='Enter Your Name',class='input')
    input(type='number',name='Age',placeholder='Enter Your Age',class='input')
    input(type='text',name='Address',placeholder='Enter Your Address',class='input')
    input(type='email',name='Email',placeholder='Enter Your Email',class='input')
    input(type='password',name='Password',placeholder='Enter a Password',class='input')
    input(type='text',name='desc',placeholder='Enter Your Description',class='input')
    button(class='btn')
      strong submit